<?xml version="1.0" encoding="utf-8"?>
<timeline>
  <name>希望の園エデン零式：再生編2</name>
  <rev>rev6</rev>
  <description>
    希望の園エデン零式： 再生編2 のスペスペたいむ定義です。
    スペスペのオプションのログフィルタにて 0x26:NetworksStatusResultList のチェックを外してください。
    0x26 のログを判定対象とするためです。
  </description>
  <author>anoyetta, sheeva</author>
  <license>CC BY-SA</license>
  <zone>947,Eden's Promise: Litany (Savage)</zone>
  <locale>JA</locale>
  <start>0039:戦闘開始！</start>

  <default target-element="Activity" target-attr="notice-o" value="-9" />

  <t name="インプロージョン：前方攻撃" sync="^14:(56F0|56FC):影の王 starts using インプロージョン on 影の王." notice="影のある方えー。" >
    <v-notice
      text="影のある方へ"
      duration="6"
      order="-1"
      icon="StackBack.png" />
  </t>

  <t name="インプロージョン：後方攻撃" sync="^14:(56F3|5700):影の王 starts using インプロージョン on 影の王." notice="影のない方えー。" >
    <v-notice
      text="影のない方へ"
      duration="6"
      order="-1"
      icon="StackFront.png" />
  </t>

  <t text="右手上げ" sync="^14:(56F4|56EA):影の王 starts using ギガスラッシュ on 影の王." notice="影の左えー">
    <v-notice
      text="影の左へ"
      duration="6"
      order="-1"
      icon="Arrow7.png" />
  </t>

  <t text="左手上げ" sync="^14:(56F8|56ED):影の王 starts using ギガスラッシュ on 影の王." notice="影の右えー">
    <v-notice
      text="影の右へ"
      duration="6"
      order="-1"
      icon="Arrow3.png" />
  </t>

  <t name="シャドウアンリーシュ" sync="影の王は「シャドウアンリーシュ」の構え。" notice="頭割り" >
    <v-notice
      text="頭割り"
      duration="6"
      order="-1"
      icon="DamageShare.png" />
  </t>

  <t name="シャドウスラッシュ" sync="影の王は「シャドウスラッシュ」の構え。" notice="直線範囲。" >
    <v-notice
      text="直線範囲"
      duration="6"
      order="-1"
      icon="Targetaoe02.png" />
  </t>

  <t name="影：上向き" sync="^26:[id8]:[mex]:[id8]:.*:0973:" >
    <expressions>
      <set name="ShadowDirection" value="N" />
    </expressions>
    <v-notice
      duration="12"
      order="-1"
      icon="Arrow1.png" />
  </t>

  <t name="影：下向き" sync="^26:[id8]:[mex]:[id8]:.*:0974:" >
    <expressions>
      <set name="ShadowDirection" value="S" />
    </expressions>
    <v-notice
      duration="12"
      order="-1"
      icon="Arrow5.png" />
  </t>

  <t name="影：右向き" sync="^26:[id8]:[mex]:[id8]:.*:0975:" >
    <expressions>
      <set name="ShadowDirection" value="E" />
    </expressions>
    <v-notice
      duration="12"
      order="-1"
      icon="Arrow3.png" />
  </t>

  <t name="影：左向き" sync="^26:[id8]:[mex]:[id8]:.*:0976:" >
    <expressions>
      <set name="ShadowDirection" value="W" />
    </expressions>
    <v-notice
      duration="12"
      order="-1"
      icon="Arrow7.png" />
  </t>

  <t name="右スラッシュ：影上向き" sync="^14:5B2C:影の王 starts using ギガスラッシュ on 影の王." notice="東側えー。" >
    <expressions>
      <pre name="ShadowDirection" value="N" />
      <set name="ShadowDirection" value="empty" />
    </expressions>
    <v-notice
      text="東側へ"
      duration="10"
      order="-2"
      icon="West.png" />
  </t>

  <t name="右スラッシュ：影下向き" sync="^14:5B2C:影の王 starts using ギガスラッシュ on 影の王." notice="西側えー。" >
