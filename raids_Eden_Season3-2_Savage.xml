  @if (Model.Player.InRole("TANK")) {
    <t name="スマッシュクリア" sync="影の王は「アンブラスマッシュ」の構え。" notice="スマッシュ">
      <expressions>
        <set name="smash_count" count="0" />
      </expressions>
      <v-notice
        text="スマッシュ"
        duration="3.5"
        duration-visible="false"
        order="-2"
        icon="HardAttack.png" />
    </t>

    <t no="1" name="スマッシュ++" sync="影の王の「アンブラスマッシュ」">
      <expressions>
        <set name="smash_count" count="+1" />
      </expressions>
    </t>

    <t no="2" name="スマッシュスイッチ" sync="影の王の「アンブラスマッシュ」" notice="スイッチ">
      <expressions>
        <pre name="smash_count" count="4" />
      </expressions>
      <v-notice
        text="スイッチ"
        duration="2"
        duration-visible="false"
        order="-2"
        icon="Switch.png" />
    </t>
  }

